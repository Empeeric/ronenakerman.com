{
  "name": "formage",
  "description": "Admin GUI addon for mongoose, jugglingdb, or just as a form generator",
  "version": "2.7.34",
  "main": "lib/index.js",
  "author": {
    "name": "Etai Peretz",
    "email": "etai@empeeric.com",
    "url": "http://www.empeeric.com/"
  },
  "contributors": [
    {
      "name": "Ishai Jaffe",
      "email": "ishai@bablic.com",
      "url": "http://www.bablic.com/"
    },
    {
      "name": "Refael Ackermann",
      "email": "me@refack.com",
      "url": "http://refack.com"
    },
    {
      "name": "Alon Valadji",
      "email": "alon@empeeric.com",
      "url": "http://www.empeeric.com/"
    }
  ],
  "dependencies": {
    "async": "0.9.0",
    "cloudinary": "1.0.10",
    "connect-multiparty": "^1.2.5",
    "debug": "1.0.3",
    "grist": "1.0.20",
    "formage-mongoose-types": "1.0.1",
    "lodash": "2.4.1",
    "lodash-contrib": "241.4.14",
    "node-ckeditor": "4.3.2",
    "nodestrum": "0.10.0",
    "socket.io": "^0.9.17"
  },
  "devDependencies": {
    "bower": "",
    "chai": "",
    "express": "< 4",
    "istanbul": "",
    "jade": "0.35.x",
    "jshint": "",
    "mocha": "",
    "mongoose": "3"
  },
  "peerDependencies": {
    "express": "< 4",
    "mongoose": ">= 3.8.0"
  },
  "keywords": [
    "admin",
    "forms",
    "cross-database",
    "MongoDB",
    "MongoDB-admin",
    "MongoDB-gui",
    "mongoose",
    "mongoose-gui",
    "juggling",
    "jugglingdb",
    "backoffice",
    "cms"
  ],
  "repository": "Empeeric/formage",
  "scripts": {
    "start": "node example/classic/app",
    "test": "npm run generate && mocha",
    "lint": "jshint .",
    "cover": "istanbul cover ./node_modules/mocha/bin/_mocha -x generated/**",
    "generate": "node utils/CompileTempletes.js",
    "setassets": "npm run generate && bower i && git clean -X -d -f assets",
    "prepublish": "npm run generate",
    "mypublish": "npm run setassets && npm version patch --version-with-shrinkwrap && git push --follow-tags"
  },
  "directories": {
    "example": "./example"
  },
  "engines": {
    "node": "^0.10.0"
  },
  "license": "MIT",
  "jshintConfig": {
    "esnext": true,
    "expr": true,
    "forin": true,
    "freeze": true,
    "globals": {
      "after": true,
      "before": true,
      "describe": true,
      "expect": true,
      "it": true,
      "Promise": false
    },
    "immed": true,
    "maxdepth": 4,
    "newcap": true,
    "noarg": true,
    "node": true,
    "noempty": true,
    "nonew": true,
    "scripturl": true,
    "sub": true,
    "supernew": true,
    "trailing": true,
    "undef": true,
    "unused": true
  }
}
